@media (width >= 768px) {
  :where(:not(:has([class*=" text-"]), :not(:has([class^="text-"])))) h1 {
    font-size: var(--bs-typo-h1-size-desktop);
  }

  :where(:not(:has([class*=" text-"]), :not(:has([class^="text-"])))) h2 {
    font-size: var(--bs-typo-h2-size-desktop);
  }

  :where(:not(:has([class*=" text-"]), :not(:has([class^="text-"])))) h3 {
    font-size: var(--bs-typo-h3-size-desktop);
  }

  :where(:not(:has([class*=" text-"]), :not(:has([class^="text-"])))) p, :where(:not(:has([class*=" text-"]), :not(:has([class^="text-"])))) button, :where(:not(:has([class*=" text-"]), :not(:has([class^="text-"])))) input {
    font-size: var(--bs-typo-body-size-desktop);
  }

  :where(:not(:has([class*=" text-"]), :not(:has([class^="text-"])))) label {
    font-size: var(--bs-typo-caption-size-desktop);
  }
}

html {
  font-size: var(--font-size);
  scroll-behavior: smooth;
}

body {
  font-family: Suisse Intl, system-ui, -apple-system, sans-serif;
}

@layer components {
  .program-flow-card {
    background: var(--bs-color-white);
    border-radius: var(--bs-program-flow-card-radius);
    padding: var(--bs-program-flow-card-padding);
    box-shadow: var(--bs-shadow-md);
    transition: all .25s ease-out;
  }

  .program-flow-card:hover {
    box-shadow: var(--bs-shadow-lg);
    transform: translateY(-4px);
  }

  .cta-gradient-bs {
    height: var(--bs-cta-height);
    background: var(--bs-gradient-arch-scale-90);
    color: var(--bs-color-white);
    border-radius: var(--bs-radius-md);
    font-weight: var(--font-weight-medium);
    border: none;
    transition: all .4s ease-out;
  }

  .cta-gradient-bs:hover {
    background: linear-gradient(90deg, var(--bs-color-orange-hover) 0%, #ff8a5c 100%);
    transform: scale(1.02);
  }

  .cta-gradient-bs:active {
    animation: .4s ease-out pulse-glow;
  }

  @keyframes pulse-glow {
    0%, 100% {
      box-shadow: 0 0 #f6782fb3;
    }

    50% {
      box-shadow: 0 0 20px 10px #c7217066;
    }
  }

  .ai-credits-bar {
    height: var(--bs-ai-credits-bar-height);
    background: #0000000d;
    border-radius: 999px;
    overflow: hidden;
  }

  .ai-credits-bar-fill {
    background: var(--bs-gradient-arch-scale-90);
    height: 100%;
    transition: width 1.2s linear;
  }

  .flow-connector {
    background: var(--bs-gradient-arch-scale-90);
    width: 2px;
    position: relative;
  }

  .flow-connector:after {
    content: "";
    border: 2px solid var(--bs-color-orange);
    background: none;
    border-radius: 50%;
    width: 16px;
    height: 16px;
    position: absolute;
    top: 50%;
    right: -8px;
    transform: translateY(-50%);
  }

  @keyframes gradient-wipe {
    0% {
      background-position: -100% 0;
    }

    100% {
      background-position: 100% 0;
    }
  }

  .flow-connector-animate {
    background: linear-gradient(90deg, transparent 0%, var(--bs-gradient-arch-scale-90) 50%, transparent 100%);
    background-size: 200% 100%;
    animation: 1.5s ease-in-out gradient-wipe;
  }

  .ai-credits-legend-pill {
    height: var(--bs-ai-credits-legend-height);
    font-family: Inter, sans-serif;
    font-size: var(--bs-typo-caption-size-desktop);
    font-weight: var(--bs-typo-caption-weight);
    border: 1px solid #8414774d;
    border-radius: 14px;
    align-items: center;
    gap: 6px;
    padding: 4px 12px;
    display: inline-flex;
  }

  .divider-bs-pink {
    background: #c7217033;
    height: 1px;
  }

  .grid-depth-plum {
    background-image: linear-gradient(#8414770d 1px, #0000 1px), linear-gradient(90deg, #8414770d 1px, #0000 1px);
    background-size: var(--bs-baseline) var(--bs-baseline);
  }
}

